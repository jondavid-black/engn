{"engn_type": "import", "modules": ["engn.project"]}
{"engn_type": "module", "name": "engn.project", "description": "Core project configuration and user schema", "files": ["engn/project.jsonl"]}
{"engn_type": "enum", "name": "Role", "description": "User roles in the system", "values": ["ADMIN", "USER", "GUEST"]}
{"engn_type": "type_def", "name": "User", "description": "Represents an authenticated user", "properties": [{"name": "id", "type": "str", "description": "Unique identifier for the user", "presence": "required"}, {"name": "email", "type": "str", "description": "Email address of the user", "presence": "required"}, {"name": "password_hash", "type": "str", "description": "Hashed password", "presence": "required"}, {"name": "roles", "type": "list[Role]", "description": "Roles assigned to the user", "presence": "optional", "default": ["USER"]}, {"name": "roles_hash", "type": "str", "description": "Hash of assigned roles", "presence": "optional"}, {"name": "theme_preference", "type": "str", "description": "UI theme preference", "presence": "optional", "default": "DARK"}, {"name": "name", "type": "str", "description": "Display name", "presence": "optional"}, {"name": "first_name", "type": "str", "description": "First name", "presence": "optional"}, {"name": "last_name", "type": "str", "description": "Last name", "presence": "optional"}, {"name": "preferred_color", "type": "str", "description": "Preferred UI color", "presence": "optional"}, {"name": "default_project", "type": "str", "description": "Default project ID", "presence": "optional"}]}
{"engn_type": "type_def", "name": "ProjectConfig", "description": "Configuration for an engn project", "properties": [{"name": "name", "type": "str", "description": "Printable name of the project", "presence": "required"}, {"name": "mbse_language", "type": "str", "description": "MBSE language used (e.g. SysML v1, SysML v2)", "presence": "required"}, {"name": "implementation_strategy", "type": "str", "description": "Implementation strategy for the project", "presence": "required"}, {"name": "pm_path", "type": "str", "description": "Path to program management content", "presence": "optional", "default": "pm"}, {"name": "sysengn_path", "type": "str", "description": "Path to MBSE content", "presence": "optional", "default": "mbse"}]}
